{
  "name": "aegis247",
  "version": "1.0.0",
  "description": "aegis location management app",
  "main": "server.js",
  "scripts": {
    "start": "nodemon clusteredApp.js",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --debug",
    "setup-db": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --debug",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "Node.js",
    "Sequelize",
    "SQL",
    "Express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1129.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^6.4.0",
    "farmhash": "^3.2.2",
    "fs": "0.0.1-security",
    "husky": "^7.0.4",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.0",
    "nodemon": "^2.0.15",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "sequelize": "^6.19.0",
    "socket.io": "^4.5.0"
  },
  "devDependencies": {
    "sequelize-cli": "^6.4.1"
  }
}
